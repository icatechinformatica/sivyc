{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3||^8.0",
        "barryvdh/laravel-dompdf": "^2.2.0",
        "doctrine/dbal": "^2.10",
        "endroid/qr-code": "4.8.1",
        "fakerphp/faker": "1.24.1",
        "fideloper/proxy": "4.4.2",
        "fruitcake/laravel-cors": "3.0.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.4",
        "lcobucci/jwt": "4.3.0",
        "maatwebsite/excel": "3.1.62",
        "revolution/laravel-google-sheets": "6.0.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.5",
        "smalot/pdfparser": "2.11.0",
        "spatie/array-to-xml": "3.4.0",
        "spatie/laravel-permission": "^6.10",
        "vyuldashev/xml-to-array": "1.1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/functionFormatoT.php",
            "app/Helpers/notificationMovil.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
