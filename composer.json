{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.0",
        "endroid/qr-code": "^4.6",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "icatech/permiso-rol-menu": "dev-feature/roles-permisos-arbol",
        "laravel/framework": "^9.0",
        "laravel/passport": "^11.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.4",
        "lcobucci/jwt": "^4.0",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^2.0",
        "revolution/laravel-google-sheets": "^6.1",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.0",
        "smalot/pdfparser": "^2.7",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-html": "^3.5",
        "vyuldashev/xml-to-array": "^1.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/Helpers/functionFormatoT.php",
            "app/Helpers/notificationMovil.php",
            "app/Helpers/FormatoTiempoHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
